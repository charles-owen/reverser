<?xml version="1.0" encoding="UTF-8" standalone="yes" ?>
<layout name="box-2-32">
    <desc>Simple 2-pin box component with 32mm spacing.</desc>
    <pins pad="4">
        <pin n="1" x="0" y="0"/>
        <pin n="2" x="32" y="0"/>
    </pins>
    <draw>
        <line x1="2" y2="0" x2="4" y1="0"/>
        <line x1="28" y2="0" x2="30" y1="0"/>
        <rect x="4" y="-3.5" width="24" height="7"/>
    </draw>

</layout>